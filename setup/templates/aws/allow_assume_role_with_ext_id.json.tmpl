{
  "Version": "2012-10-17",
    "Statement": [
      {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::$parent_account_id:user/$parent_account_username"
      },
      "Action": "sts:AssumeRole",
      "Condition": {
        "StringEquals": {
        "sts:ExternalId": "$external_id"
        }
      }
    }
  ]
}
